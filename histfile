    1  sudo apt-get install git vim-nox
    2  git clone https://github.com/sigurdga/gnome-terminal-colors-solarized.git
    3  cd gnome-terminal-colors-solarized/
    4  ./set_light.sh 
    5  ls --color
    6  ll
    7  vim .bashrc
    8  cd
    9  vim .bashrc
   10  sudo apt-get update
   11  sudo apt-get upgrade
   12  echo $TERM
   13  export TERM=xterm-256color
   14  echo $TERM
   15  wget --no-check-certificate https://raw.github.com/seebi/dircolors-solarized/master/dircolors.ansi-light
   16  wget --no-check-certificate https://raw.github.com/seebi/dircolors-solarized/master/dircolors.ansi-dark
   17  ls
   18  ll
   19  cp dircolors.ansi-light .dircolors
   20  eval `dircolors ~/.dircolors`
   21  ls --color
   22  ls
   23  git clone git://github.com/stever227/.vim
   24  ln -s ./.vim/.vimrc .vimrc
   25  vim .vimrc
   26  exit
   27  ps
   28  startx
   29  ls
   30  ls -al
   31  cat .xsession-errors 
   32  cat .xsession-errors  | less
   33  sudo shutdown now -h
   34  sudo apt-get update
   35  sudo apt-get upgrade
   36  sudo shutdown now -h
   37  cd /boot
   38  ls
   39  cd grub
   40  ls
   41  cd /etc/grub.d/
   42  ls
   43  find / -name menu.lst
   44  startx
   45  cd
   46  ls -al
   47  history
   48  cd .vim
   49  ls
   50  history > histfile
